{
  "name": "@raydeck/serverless-yaml",
  "version": "1.4.0",
  "description": "Inherit YAML changes for serverless",
  "main": "lib/index.js",
  "bin": "lib/bin.js",
  "scripts": {
    "pre-commit": "(git-branch-is --not main || (set -u && echo $ALLOWMAIN)) && yarn test",
    "test": "tsc --noEmit",
    "build": "tsc && chmod +x lib/bin.js && yarn pack -f package.tgz",
    "docs": "typedoc --entryPoints src/index.ts --theme markdown --readme none  --includeVersion --out docstemp && commanderdoc markdown lib/bin.js -o docstemp/__cli.md --cli-name \"npx `jq -r .name package.json`\" && rm README.md ; mv docstemp/README.md docstemp/_library.md && cp TOP.md docstemp/README.md ; concat-md docstemp > README.md && rm -rf docstemp",
    "prepublishOnly": "yarn build && yarn docs && git commit README.md -m \"docs: updating docs\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhdeck/serverless-yaml.git"
  },
  "author": "Ray Deck",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhdeck/serverless-yaml/issues"
  },
  "homepage": "https://github.com/rhdeck/serverless-yaml#readme",
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/node": "^14.0.27",
    "commanderdoc": "^0.1.11",
    "concat-md": "^0.3.5",
    "git-branch-is": "^4.0.0",
    "husky": "^4.2.5",
    "typedoc": "^0.20.16",
    "typedoc-plugin-markdown": "^3.4.0",
    "typescript": "^4.1.3"
  },
  "files": [
    "src",
    "lib"
  ],
  "dependencies": {
    "@raydeck/livelink": "^1.5.2",
    "@raydeck/serverless-base": "^1.0.4",
    "commander": "^6.0.0",
    "install-peers-cli": "^2.2.0",
    "yaml": "^1.10.0"
  },
  "liveLink": {
    "ignoreMasks": [
      "lib/*",
      "package.tgz"
    ]
  }
}
